{
  "stage": 1,
  "env": {
    "start": {
      "plugins": [
        "react-transform"
      ],
      "extra": {
        "react-transform": {
          "transforms": [
            {
              "transform": "react-transform-hmr",
              "imports": ["react","redbox-react"],
              "locals": ["module"]
            },
            {
              "transform":"react-transform-catch-errors",
              "imports": ["react","redbox-react","./reporterOptions"],
              "locals": ["module"]
            }
          ]
        }
      }
    }
  }
}
